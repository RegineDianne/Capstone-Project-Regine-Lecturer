        <activity android:name="com.capstoneproject.regine_lecturer.Regine_Lecturer_OrderDetail" />
        <activity android:name="com.capstoneproject.regine_lecturer.Regine_Lecturer_OrderActivity" />
        <activity android:name="com.capstoneproject.regine_lecturer.Regine_Lecturer_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
